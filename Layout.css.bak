			.leg1{
			   margin-bottom: 0;
			   margin-left: 0;
			   background-color: white;
			   color:black;
			   font: bold Helvetica;
			   border-radius: 10px;
			   padding: 5px;
			}
			input [type= text], select{
			   width: 80%;
			   padding:12px 20px;
			   margin:8px;
			   display: inline-block;
			   border: 1px solid #ccc;
			   border-radius:5px;
			   box-sizing: border-box;
			}
			input[type= button]{
			   background:white;
			   color:black;
			   border-style:outset;
			   border-radius:6px;
			   font:bold 15px arial sans-serif;
			   text-shadow:none;
			}
			input[type = button]:hover{
			  background-color:#EDBC19;
			  border-style:outset;
			  border-color:white;
			  cursor:pointer;
